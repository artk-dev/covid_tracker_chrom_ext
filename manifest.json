{
    "name":"Covid19tracker",
    "version":"0.1",
    "manifest_version":2,
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["track.js"]
        }
    ],
    "browser_action":{
        "default_popup":"box.html",
        "default_title":"Covid Tracker"
    },
    "content_security_policy": "script-src-elem 'self' https://platform.twitter.com https://cdn.syndication.twimg.com; object-src 'self'"
}